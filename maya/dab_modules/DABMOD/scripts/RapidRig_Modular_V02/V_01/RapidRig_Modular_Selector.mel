					}				
					
					
					//ARM
					if ($moduleType == "arm")
					{
						if ($eachSideChannel == "translateX" || $eachSideChannel == "rotateY" || $eachSideChannel == "rotateZ")
						{
							setAttr ($eachLeftControl + "." + $eachSideChannel) ($rightVal * -1);
