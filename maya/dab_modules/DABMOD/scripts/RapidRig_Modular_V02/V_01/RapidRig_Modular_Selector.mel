					}
				}
				//AUXILIARY
				if ($moduleType == "auxiliary")
				{
					if ($eachSideChannel == "translateX" || $eachSideChannel == "rotateZ" || $eachSideChannel == "rotateY")
					{
						setAttr ($eachRightControl + "." + $eachSideChannel) ($leftVal * -1);
